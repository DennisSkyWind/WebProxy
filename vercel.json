{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/proxy/(.*)",
      "dest": "/api/proxy?url=$1"
    }
  ],
  "env": {
    "HOST": "localhost",
    "PORT": 3000
  },
  "functions": {
    "api/proxy": {
      "handler": "index.js",
      "memory": 512
    }
  }
}
